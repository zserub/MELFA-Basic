{
  "name": "melfa-basic",
  "displayName": "MELFA-Basic",
  "description": "Mitsubishi robot programming language support",
  "version": "0.0.4",
  "publisher": "zserub",
  "repository": {
    "url": "https://github.com/zserub/MELFA-Basic"
  },
  "license": "MIT",
  "engines": {
    "vscode": "1.83.1"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Themes"
  ],
  "keywords": [
    "language support",
    "syntax highlight",
    "formatter",
    "snippets",
    "script",
    "MELFA",
    "Mitsubishi",
    "robot programming language"
  ],
  "contributes": {
    "languages": [
      {
        "id": "prg",
        "extensions": [
          ".prg"
        ],
        "aliases": [
          "MELFA-Basic",
          "prg"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "prg",
        "scopeName": "source.prg",
        "path": "./syntaxes/prg.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "prg",
        "path": "./snippets/snippets.code-snippets"
      }
    ],
    "themes": [
      {
        "label": "Dark Theme for MELFA-Basic",
        "uiTheme": "vs-dark",
        "path": "./themes/Dark Theme for MELFA-Basic-color-theme.json"
      }
    ]
  }
}